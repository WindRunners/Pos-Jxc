.modal.fade#addDicModal{tabindex:'-1',role:'dialog','aria-labelledby'=>'addDicModalLabel'}
  .modal-dialog{role:'document'}
    .modal-content
      .modal-header
        %button{type:'button',class:'close','data-dismiss'=>'modal','aria-label'=>'Close'}
          %span{'aria-hidden'=>'true'}
            &times;
        %h4{class:'modal-title',id:'selectModalLabel'}
      .modal-body
      .modal-footer
        %button{type:'button',class:'btn btn-default',id:'close-modal','data-dismiss'=>'modal'}关闭
        %button{type:'button',class:'btn btn-primary',id:'confirm-button','data-type'=>'',onclick:'submit()'}确定