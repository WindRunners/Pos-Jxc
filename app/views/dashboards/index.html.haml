.row
  .col-xs-12
    .row
      /当月商品曝光量排名
      .col-sm-6#exposure_product_container
      /近几日各门店有效订单量统计
      .col-sm-6#store_order_container
  .col-xs-12
    .row
      /商品销售比例
      .col-sm-6#sale_product_data_container
      .col-sm-6.infobox-container
        /绿色主题
        .infobox.infobox-green
          .infobox-icon
            %i.ace-icon.fa.fa-comments
          .infobox-data
            %span.infobox-data-number 32
            .infobox-content comments + 2 reviews
          .stat.stat-success 8%
        /蓝色主题
        .infobox.infobox-blue
          .infobox-icon
            %i.ace-icon.fa.fa-twitter
          .infobox-data
            %span.infobox-data-number 11
            .infobox-content new followers
          .stat.stat-success
            32%
            %i.ace-icon.fa.fa-arrow-up


