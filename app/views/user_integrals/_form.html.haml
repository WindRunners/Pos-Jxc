= form_for @user_integral do |f|
  - if @user_integral.errors.any?
    #error_explanation
      %h2= "#{pluralize(@user_integral.errors.count, "error")} prohibited this user_integral from being saved:"
      %ul
        - @user_integral.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :integral
    = f.text_field :integral
  .field
    = f.label :state
    = f.text_field :state
  .field
    = f.label :type
    = f.text_field :type
  .actions
    = f.submit 'Save'
