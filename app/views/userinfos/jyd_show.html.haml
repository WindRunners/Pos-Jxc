%p
  %b 公司名称:
  = @userinfo.shopname
%p
  %b 法人姓名:
  = @userinfo.name
%p
  %b 法人身份证号:
  = @userinfo.idnumber
%p
  %b 公司地址:
  = @userinfo.address
%p
  %b 法人手机号码:
  = @userinfo.pusher_phone
%p
  %b 公司状态:
  = @userinfo.status_str
%p
  %b 默认运费:
  = @userinfo.fright
%p
  %b 默认起送价格:
  = @userinfo.lowestprice
%p
  %b 预警库存数量:
  = @userinfo.alarm_stock
%p
= link_to '编辑',"",'data-href' =>'/userinfos/jyd_edit/'+ @userinfo.id,:class => 'btn btn-info'
\|
-if current_user.has_role?(:SuperAdmin)
  = link_to '返回',"",'data-href' =>jyd_index_userinfos_path,:class => 'btn btn-info'
-else
  = link_to '返回',"",'data-href' => '/userinfos/jyd_edit/'+@userinfo.id,:class => 'btn btn-info'